{
  "name": "palstek",
  "version": "0.5.2",
  "description": "A bunch of useful extensions for react projects",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "ava",
    "lint": "eslint",
    "check": "tsc --noEmit",
    "pub": "npm run lint && npm run check && npm run build && np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmmdmx/palstek.git"
  },
  "keywords": [
    "react",
    "utils",
    "javascript",
    "typescript",
    "library"
  ],
  "author": "@cmmdmx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmmdmx/palstek/issues"
  },
  "homepage": "https://github.com/cmmdmx/palstek#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.6.0",
    "@parcel/transformer-typescript-types": "^2.6.0",
    "@types/react": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "ava": "^4.3.0",
    "eslint": "^8.17.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-react": "^7.30.0",
    "np": "^7.6.1",
    "parcel": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 12"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "ava/no-ignored-test-files": [
      "error"
    ]
  },
  "staticFiles": {
    "staticPath": "src/static"
  }
}
