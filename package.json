{
  "name": "palstek",
  "version": "0.5.2",
  "description": "A bunch of useful extensions for react projects",
  "files": [
    "dist"
  ],
  "main": "./dist/palstek.umd.js",
  "module": "dist/palstek.es.js",
  "typings": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/palstek.es.js",
      "require": "./dist/palstek.umd.js"
    }
  },
  "source": "src/index.ts",
  "scripts": {
    "build": "tsc --build && vite build && tsc",
    "test": "ava",
    "lint": "eslint",
    "pub": "npm run lint && npm run build && np"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmmdmx/palstek.git"
  },
  "keywords": [
    "react",
    "utils",
    "javascript",
    "typescript",
    "library"
  ],
  "author": "@cmmdmx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmmdmx/palstek/issues"
  },
  "homepage": "https://github.com/cmmdmx/palstek#readme",
  "devDependencies": {
    "@types/react": "^17.0.45",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@vitejs/plugin-react": "^1.3.2",
    "ava": "^4.3.0",
    "eslint": "^8.17.0",
    "eslint-plugin-ava": "^13.2.0",
    "eslint-plugin-react": "^7.30.0",
    "np": "^7.6.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "vite": "^2.9.9"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 12"
  },
  "peerDependencies": {
    "react": "^17.0.2 || ^18.0.0"
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "staticFiles": {
    "staticPath": "src/static"
  }
}
