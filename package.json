{
  "name": "palstek",
  "version": "0.1.0",
  "description": "A bunch of useful extensions for react projects",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "types": "dist/types.d.ts",
  "source": "src/index.ts",
  "scripts": {
    "watch": "parcel watch",
    "build": "parcel build",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmmdmx/palstek.git"
  },
  "keywords": [
    "react",
    "utils",
    "javascript",
    "typescript",
    "library"
  ],
  "author": "@cmmdmx",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cmmdmx/palstek/issues"
  },
  "homepage": "https://github.com/cmmdmx/palstek#readme",
  "devDependencies": {
    "@parcel/packager-ts": "^2.0.0",
    "@parcel/transformer-typescript-types": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "ava": "^3.15.0",
    "eslint": "^8.0.1",
    "eslint-plugin-ava": "^13.0.0",
    "eslint-plugin-react": "^7.26.1",
    "parcel": "^2.0.0",
    "typescript": "^4.4.4"
  },
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "engines": {
    "node": ">= 12"
  },
  "ava": {
		"extensions": [
			"ts"
		],
		"require": [
			"ts-node/register"
		],
    "ava/no-ignored-test-files": ["error"]
	}
}
